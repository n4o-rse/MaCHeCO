<http://www.cidoc-crm.org/cidoc-crm/P195i_had_presence> a rdf:Property ;
    rdfs:label "had presence"@en,
        "a eu pour présence"@fr,
        "имeл присутствиe"@ru ;
    rdfs:domain <http://www.cidoc-crm.org/cidoc-crm/E18_Physical_Thing> ;
